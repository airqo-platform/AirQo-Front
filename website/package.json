    "@mui/material": "^5.11.8",
    "axios": "^1.3.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "i18next": "^23.5.1",
    "i18next-http-backend": "^2.2.2",
    "mini-css-extract-plugin": "^2.9.0",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-player": "^2.12.0",
    "react-router-dom": "^6.8.1",
    "react-scroll": "^1.8.9",
