    "postcss-preset-env": "^9.6.0",
    "prettier": "^3.3.3",
    "react-i18next": "^15.0.0",
    "react-redux": "^9.1.2",
    "react-test-renderer": "^18.2.0",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "sass-loader": "^16.0.0",
    "style-loader": "^4.0.0",
